     <visual>
       <geometry>
         <mesh filename="package://solid3/meshes/base_link.STL"/>
       </geometry>
       <material name="">
         <color rgba="0.666667 0.698039 0.768627 1" />
       </material>
       <origin xyz="0 0 0" rpy="0 -0 0"/>
     </visual>
     <collision>
