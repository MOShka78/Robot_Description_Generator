       </geometry>
       <origin xyz="0 0 0" rpy="0 -0 0"/>
     </collision>
     <inertial>
       <origin xyz="0 0 0" rpy="0 0 0"/>
       <mass value="${base_link_mass}"/>
       <inertia ixx="0.239137" ixy="-0.000543649" ixz="-0.0422495" iyy="0.822805" iyz="0.0168714" izz="0.650319"/>
     </inertial>
   </link>


